<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(wallpaper) -%>">
  <span class='title'>
    <%= wallpaper.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.wallpapers_wallpaper_path(wallpaper),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_wallpapers_admin_wallpaper_path(wallpaper),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.wallpapers_admin_wallpaper_path(wallpaper),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => wallpaper.title),
        :method => :delete %>
  </span>
</li>
